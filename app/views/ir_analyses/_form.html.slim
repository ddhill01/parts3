  div
    = form.label :WOComplete, style: "display: block"
    = form.check_box :WOComplete

  div
    = form.label :WOCreated, style: "display: block"
    = form.check_box :WOCreated

  div
    = form.label :WONumber, style: "display: block"
    = form.text_field :WONumber

  div
    = form.label :WOPriority, style: "display: block"
    = form.number_field :WOPriority

  div
    = form.label :VoltageL12, style: "display: block"
    = form.text_field :VoltageL12

  div
    = form.label :VoltageL23, style: "display: block"
    = form.text_field :VoltageL23

  div
    = form.label :VoltageL31, style: "display: block"
    = form.text_field :VoltageL31

  div
    = form.label :VoltageL1G, style: "display: block"
