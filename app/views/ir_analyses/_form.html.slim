  div
    = form.label :BPhaseAmps, style: "display: block"
    = form.text_field :BPhaseAmps

  div
    = form.label :CPhaseAmps, style: "display: block"
    = form.text_field :CPhaseAmps

  div
    = form.label :Loading, style: "display: block"
    = form.text_field :Loading

  div
    = form.label :WasFaultCorrectedByPdM, style: "display: block"
