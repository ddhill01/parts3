= form_with(model: @ir_analysis) do |form|
  - if @ir_analysis.errors.any?
    div style="color: red"
      h2 = "#{pluralize(@ir_analysis.errors.count, "error")} prohibited this ir_analysis from being saved:"
      ul
        - @ir_analysis.errors.each do |error|
          li = error.full_message

  div
    = form.label :AssetID, style: "display: block"
    = form.number_field :AssetID

  div
    = form.label :AssetBucketID, style: "display: block"
    = form.number_field :AssetBucketID

  div
    = form.label :IRFaultID, style: "display: block"
    = form.number_field :IRFaultID

  div
    = form.label :dbUserID, style: "display: block"
    = form.number_field :dbUserID

  div
    = form.label :IRRepairDescriptionID, style: "display: block"
    = form.number_field :IRRepairDescriptionID

  div
    = form.label :IRDefectDecriptionID, style: "display: block"
    = form.number_field :IRDefectDecriptionID

  div
    = form.label :DateTested, style: "display: block"
    = form.datetime_field :DateTested

  div
    = form.label :DateAnalyzed, style: "display: block"
    = form.datetime_field :DateAnalyzed

  div
    = form.label :AnalysisComments, style: "display: block"
    = form.text_area :AnalysisComments

  div
    = form.label :InternalComments, style: "display: block"
    = form.text_area :InternalComments

  div
    = form.label :IsFlaggedForReview, style: "display: block"
    = form.check_box :IsFlaggedForReview

  div
    = form.label :IRComponentID, style: "display: block"
    = form.number_field :IRComponentID

  div
    = form.label :IRFaultLocationID, style: "display: block"
    = form.number_field :IRFaultLocationID

  div
    = form.label :IRPhaseWireID, style: "display: block"
