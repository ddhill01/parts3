  div
    = form.label :EmailedToPlants, style: "display: block"
    = form.check_box :EmailedToPlants

  div
    = form.label :EmailedToPdM, style: "display: block"
    = form.check_box :EmailedToPdM

  div
    = form.label :DeltaTinF, style: "display: block"
    = form.text_field :DeltaTinF

  div
    = form.label :ReportForReferenceOnly, style: "display: block"
    = form.check_box :ReportForReferenceOnly

  div
    = form.submit
